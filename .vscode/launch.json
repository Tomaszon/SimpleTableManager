{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Build n Launch",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceFolder}/SimpleTableManager/SimpleTableManager/bin/Debug/net8.0/SimpleTableManager.dll",
			"console": "externalTerminal",
			"args": [],
			"cwd": "${workspaceFolder}/SimpleTableManager/SimpleTableManager",
			"stopAtEntry": false,
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"sourceFileMap": {
				"/Views": "${workspaceFolder}/SimpleTableManager/SimpleTableManager/Views"
			},
			"justMyCode": true
		},
		{
			
			"name": "Launch",
			"type": "coreclr",
			"request": "launch",
			"program": "${workspaceFolder}/SimpleTableManager/SimpleTableManager/bin/Debug/net8.0/SimpleTableManager.dll",
			"console": "externalTerminal",
			"args": [],
			"cwd": "${workspaceFolder}/SimpleTableManager/SimpleTableManager",
			"stopAtEntry": false,
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"sourceFileMap": {
				"/Views": "${workspaceFolder}/SimpleTableManager/SimpleTableManager/Views"
			},
			"justMyCode": true
		},
		{
			"name": "Prod Build n Launch",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "buildRelease",
			"program": "${workspaceFolder}/SimpleTableManager/SimpleTableManager/bin/release/net8.0/SimpleTableManager.dll",
			"console": "externalTerminal",
			"args": [],
			"cwd": "${workspaceFolder}/SimpleTableManager/SimpleTableManager",
			"stopAtEntry": false,
			"env": {
				"ASPNETCORE_ENVIRONMENT": "Production"
